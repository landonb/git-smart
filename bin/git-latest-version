# Prints latest version tag.

# +++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ #

source_deps () {
  # Load git_latest_version_tag.
  . "$(dirname "$(realpath "$0")")/../deps/sh-git-nubs/bin/git-nubs.sh"
}

# ***

main () {
  source_deps

  git_latest_version_tag
}

if [ "$0" = "${BASH_SOURCE[0]}" ]; then
  main "${@}"
fi

